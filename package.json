{
  "name": "node-sqlite-api",
  "version": "1.0.0",
  "description": "A simple Node.js API service using SQLite with Docker",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "db:seed": "node migrations/seed.js",
    "db:migrate": "node migrations/init.js",
    "generate": "npm install && npm run db:migrate && npm run db:seed",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "sequelize": "^6.3.5",
    "sqlite": "^5.1.1",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "better-sqlite3": "^11.3.0",
    "cypress": "^13.15.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "sqlite3": "^5.1.7"
  }
}
